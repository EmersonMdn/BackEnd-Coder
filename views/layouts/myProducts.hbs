<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>My products</title>
  </head>
  <body>
    <table>
      <thead>
        <tr>
          <th>Productos</th>
          <th>Precio</th>
          <th>Thumbnail</th>
        </tr>
      </thead>

      <tbody>
        {{#each items}}
          <tr>
            <td>{{this.title}}</td>
            <td>{{this.price}}</td>
            <td>{{this.thumbnail}}</td>
          </tr>
        {{/each}}
      </tbody>
    </table>
  </body>
</html>